<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- 包含SharedPreferences数据 -->
        <include domain="sharedpref" path="."/>
        <!-- 排除敏感数据 -->
        <exclude domain="sharedpref" path="sensitive_data.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- 包含SharedPreferences数据 -->
        <include domain="sharedpref" path="."/>
        <!-- 排除敏感数据 -->
        <exclude domain="sharedpref" path="sensitive_data.xml"/>
    </device-transfer>
</data-extraction-rules>